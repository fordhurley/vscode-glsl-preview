{
	"name": "glsl-preview",
	"displayName": "GLSL Preview",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.31.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:glslPreview.show"
	],
	"main": "./out/main.js",
	"contributes": {
		"commands": [{
			"command": "glslPreview.show",
			"title": "Show GLSL Preview",
			"when": "editorLangId == glsl"
		}]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21"
	}
}
